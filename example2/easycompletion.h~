#ifndef _EASYCOMPLETION_H
#define _EASYCOMPLETION_H 1

#include <gtkmm>
#include <iostream>

class EasyCompletion : public Gtk::EntryCompletion
{
  EntryCompletion()
    {
      std::cout << "CREO COMPLETION" << std::endl;
    }

  virtual ~EntryCompletion()
    {
      std::cout << "DESTROYY"<<std::endl;
    }

}

#endif /* _EASYCOMPLETION_H */

